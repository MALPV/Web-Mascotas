@model Negocio.EMascota

<h3>Eliminar Mascota</h3>
<div>
    <h4>¿Esta seguro que desea eliminar?</h4>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            Nombre Mascota
        </dt>

        <dd>
            @Html.DisplayFor(model => model.nomMascota)
        </dd>

        <dt>
            Raza
        </dt>

        <dd>
            @Html.DisplayFor(model => model.raza)
        </dd>

        <dt>
            Edad
        </dt>

        <dd>
            @Html.DisplayFor(model => model.edad)
        </dd>

        <dt>
            Sexo
        </dt>

        <dd>
            @Html.DisplayFor(model => model.sexo)
        </dd>

    </dl>

    @using (Html.BeginForm()) {
        @Html.AntiForgeryToken()

        <div class="form-actions no-color">
            <input type="submit" value="Borrar" class="btn btn-default bottom-right" /> |
            @Html.ActionLink("Volver a la lista", "List")
        </div>
    }
</div>
